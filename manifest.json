{
  "name": "Công cụ nhập hàng Taobao-Order.com",
  "version": "1.5",
  "description": "Order Việt-Trung cung cấp công cụ hỗ trợ bạn đặt hàng trực tuyến tại các website thương mại điện tử như TMALL, Taobao.",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "css": [
        "css/font-awesome.min.css",
        "css/main.css",
        "css/sweetalert.min.css"
      ],
      "matches": [
        "https://datn.order-taobao.com/*",
        "http://datn.order-taobao.com/*",
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "http://*.tmall.hk/*",
        "https://*.tmall.hk/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "vendor/sweetalert2.min.js",
        "vendor/helper.js",
        "config.js",
        "taobao.js",
        "tmall.js",
        "alibaba.js",
        "tool.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "system.display",
    "system.cpu",
    "system.memory",
    "cookies"
  ],
  "host_permissions": [
    "https://datn.order-taobao.com/*",
    "http://datn.order-taobao.com/*",
    "https://translate.google.com/*",
    "http://taobao-order.com/*",
    "https://taobao-order.com/*",
    "http://*.taobao.com/*",
    "https://*.taobao.com/*",
    "http://*.tmall.com/*",
    "https://*.tmall.com/*",
    "http://*.tmall.hk/*",
    "https://*.tmall.hk/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "css/sweetalert.min.css",
        "vendor/sweetalert2.min.js",
        "template/index.html",
        "css/font-awesome.min.css",
        "css/main.css",
        "css/cn_main.css",
        "img/cart_ico.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    }
  }
}
